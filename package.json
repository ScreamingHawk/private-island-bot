{
  "name": "private-island-bot",
  "version": "1.0.0",
  "description": "A discord bot for moderating the Private Island server",
  "private": true,
  "main": "dist/index.js",
  "author": "Michael Standen",
  "license": "MIT",
  "repository": "git@github.com:ScreamingHawk/private-island-bot.git",
  "scripts": {
    "build": "tsc",
    "dev": "bun --watch run src/index.ts",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "node dist/index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "discord.js": "^14.16.3",
    "dotenv": "^8.2.0",
    "lefthook": "^1.8.2",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "nodemon": "^2.0.4",
    "prettier": "^3.3.3",
    "prettier-plugin-organize-imports": "^4.1.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}
